{
  "directories": {
    "test": "test"
  },
  "author": "azu",
  "license": "MIT",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "name": "github-longest-streak",
  "version": "1.0.0",
  "description": "Online Viewer for GitHub longest longest_streak",
  "main": "lib/github-longest-streak.js",
  "scripts": {
    "test": "mocha test/",
    "prepublish": "npm run --if-present build",
    "start": "npm-run-all clean --parallel watch:css server",
    "server": "NODE_ENV=development webpack-dev-server -d --hot --content-base ./public --open",
    "build:js": "NODE_ENV=production webpack -p",
    "build:css": "postcss --config postcss.config.js",
    "build": "npm-run-all clean --parallel build:*",
    "watch:js": "NODE_ENV=development webpack -d --watch",
    "watch:css": "chokidar --initial 'src/**/*.css' -c 'postcss --config postcss.config.js'",
    "watch": "npm-run-all clean --parallel watch:*",
    "clean": "rimraf public/build && mkdirp public/build",
    "deploy": "npm run build && git subtree push --prefix public/ origin gh-pages"
  },
  "keywords": [
    "github",
    "stats"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/github-longest-streak.git"
  },
  "bugs": {
    "url": "https://github.com/azu/github-longest-streak/issues"
  },
  "homepage": "https://github.com/azu/github-longest-streak",
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-jsdoc-to-assert": "^4.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chokidar-cli": "^1.2.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "postcss-calc": "^5.3.1",
    "postcss-cli": "^2.6.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-easy-import": "^1.0.1",
    "postcss-reporter": "^3.0.0",
    "power-assert": "^1.4.2",
    "rimraf": "^2.5.4",
    "webpack": "beta",
    "webpack-dev-server": "beta"
  },
  "dependencies": {
    "github-calendar-parser": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-parse": "0.0.3",
    "moment": "^2.17.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  }
}
